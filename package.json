{
  "name": "nuxt-xstate",
  "version": "0.3.1",
  "license": "MIT",
  "type": "module",
  "repository": "Lexpeartha/nuxt-xstate",
  "description": "XState integration for Nuxt",
  "author": "Lexpeartha",
  "keywords": [
    "nuxt",
    "module",
    "state",
    "nuxt-module",
    "xstate",
    "xstate-library",
    "state-machines",
    "web"
  ],
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:generate": "nuxi generate playground",
    "dev:preview": "nuxi preview playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "docs:dev": "vitepress dev docs --port 4000",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:types": "tsc --noEmit",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@nuxt/kit": "^3.0.0-rc.9",
    "@xstate/fsm": "^2.0.0",
    "@xstate/vue": "^2.0.0",
    "globby": "^13.1.2",
    "pathe": "^0.3.4",
    "unimport": "^0.6.7",
    "xstate": "^4.33.5"
  },
  "devDependencies": {
    "@nuxt/module-builder": "latest",
    "@nuxt/schema": "^3.0.0-rc.9",
    "@nuxt/test-utils-edge": "^3.0.0-rc.9",
    "@nuxt/vite-builder": "^3.0.0-rc.9",
    "@nuxt/webpack-builder": "^3.0.0-rc.9",
    "@nuxtjs/eslint-config-typescript": "10.0.0",
    "eslint": "latest",
    "nuxt": "^3.0.0-rc.9",
    "playwright": "^1.25.0",
    "typescript": "^4.7.4",
    "vitepress": "v1.0.0-alpha.13",
    "vitest": "^0.22.0",
    "vue": "^3.2.37",
    "vue-demi": "^0.13.8",
    "vue-router": "^4.1.5"
  }
}
